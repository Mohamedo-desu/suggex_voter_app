{
  "name": "live_voting_app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "publish:android": "eas build --platform android --profile preview",
    "update:app": "eas update --branch preview && npx sentry-expo-upload-sourcemaps dist",
    "upgrade": "bun install expo@latest && bunx expo install --fix",
    "android": "bunx expo run:android --device",
    "build:web": "npx expo export --platform web",
    "release:android": "bunx expo run:android --device --variant Release",
    "release:web": "eas deploy --prod",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@clerk/clerk-expo": "^2.10.7",
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-native-picker/picker": "2.11.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@sentry/react-native": "~6.10.0",
    "convex": "^1.24.0",
    "date-fns": "^4.1.0",
    "expo": "~53.0.6",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.5",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.6",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.4",
    "expo-router": "~5.0.5",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-updates": "~0.28.12",
    "expo-web-browser": "~14.1.6",
    "formik": "^2.4.6",
    "nanoid": "^5.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-animated-numbers": "^0.6.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "svix": "^1.64.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "trustedDependencies": [
    "@clerk/shared",
    "@sentry/cli",
    "browser-tabs-lock",
    "core-js",
    "unrs-resolver"
  ],
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "@babel/plugin-proposal-export-namespace-from",
          "convex",
          "nanoid",
          "svix",
          "yup",
          "react-native-awesome-alerts",
          "react-native-responsive-fontsize"
        ],
        "listUnknownPackages": true
      }
    }
  }
}
