{
  "name": "suggestion_box_app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "publish:android": "eas build --platform android --profile preview",
    "update:app": "eas update --branch preview && npx sentry-expo-upload-sourcemaps dist",
    "upgrade": "bun install expo@latest && bunx expo install --fix",
    "android": "bunx expo run:android --device",
    "build:web": "npx expo export --platform web",
    "release:android": "bunx expo run:android --device --variant Release",
    "release:web": "eas deploy --prod",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@clerk/clerk-expo": "^2.9.7",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^5",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@sentry/react-native": "~6.3.0",
    "convex": "^1.23.0",
    "date-fns": "^4.1.0",
    "expo": "~52.0.43",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-image": "~2.0.7",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-navigation-bar": "~4.0.9",
    "expo-router": "~4.0.20",
    "expo-secure-store": "^14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "formik": "^2.4.6",
    "nanoid": "^5.1.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-animated-numbers": "^0.6.3",
    "react-native-awesome-alerts": "^2.0.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "svix": "^1.63.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true,
  "trustedDependencies": [
    "@clerk/shared",
    "@sentry/cli",
    "browser-tabs-lock",
    "core-js",
    "esbuild"
  ],
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "@babel/plugin-proposal-export-namespace-from",
          "convex",
          "nanoid",
          "svix",
          "yup",
          "react-native-awesome-alerts",
          "react-native-responsive-fontsize"
        ],
        "listUnknownPackages": true
      }
    }
  }
}
